                                    <input type="number" id="phoneNumber" name="phoneNumber" class="form-control"
                                        placeholder="Enter Phone Number" required />
                                </div>
                                <div class="form-group">
                                    <label for="subject">Subject</label>
                                    <input type="text" id="subject" name="subject" class="form-control"
                                        placeholder="Enter Subject"
                                        value="<%= typeof name != 'undefined' ? name : '' %>" required />
                                </div>
                                <div class="form-group">
                                    <label for="message">Message</label>
                                    <textarea id="message" name="message" class="form-control" placeholder="Message"
                                        value="<%= typeof message != 'undefined' ? message : '' %>"
                                        style="height: 100%;width: 100%;" required></textarea>
                                </div>
                                <button type="submit" class="btn btn-primary btn-block" style="border-radius: 25px;">
                                    Submit
