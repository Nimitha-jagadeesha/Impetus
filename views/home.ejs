      border-top: 5px solid rgb(70, 70, 70);
      border-bottom: 5px solid rgb(80, 80, 80);
      animation: clockwisespin 5s linear 3;
    }

    @keyframes clockwisespin {
      from {
        transform: rotate(0deg);
      }

      to {
        transform: rotate(360deg);
      }
    }

    @keyframes fadeout {
      from {
        opacity: 1;
      }

      to {
        opacity: 0;
      }
    }

    @keyframes fadein {
      from {
        opacity: 0;
      }

      to {
        opacity: 1;
      }
    }
