                            <form action="/register" method="POST">
                                <div class="form-group">
                                    <label for="name">Name</label>
                                    <input type="name" id="name" name="name" class="form-control"
                                        placeholder="Enter Name" value="<%= typeof name != 'undefined' ? name : '' %>"
